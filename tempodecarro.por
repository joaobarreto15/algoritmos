programa {
  funcao inicio() {
    inteiro velocidade , distancia , tempo
    escreva ("Escreva sua velocidade em kmh :")
    leia(velocidade)
    escreva ("Escreva a distancia percorrida em km :")
    leia(distancia)
    tempo = distancia / velocidade
    
    escreva ("O tempo necessario é de :" , tempo , "horas")
    
  }
}
